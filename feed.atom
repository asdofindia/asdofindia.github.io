<?xml version="1.0" encoding="utf-8"?>

<feed xmlns="http://www.w3.org/2005/Atom">

    <title type="text">Akshay S Dinesh</title>
    <subtitle type="text">Thinker, blogger, coder, learner, (to be) doctor</subtitle>
    <id>http://asd.learnlearn.in/feed.atom</id>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <link href="http://asd.learnlearn.in/feed.atom" rel="self" type="application/"/>
    <link href="http://asd.learnlearn.in/" hreflang="en" rel="alternate" type="text/html"/>
    <rights>Copyright (c) Akshay S Dinesh, released under CC-BY-SA 4.0 license</rights>
    <generator uri="https://github.com/asdofindia/asdofindia.github.io/blob/master/feed.atom" version="1.0">
        Jekyll feed template
    </generator>
    {% for post in site.posts limit:20 %}
    <entry>
        <title>{{ post.title }}</title>
        {% if post.externalLink %}
        <link href="{{ post.externalLink }}"/>
        {% else %}
        <link href="{{ site.url }}{{ post.url }}" rel="alternate" type="text/html"/>
        {% endif %}
        <id>{{ site.url }}{{ post.url }}</id>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <author>
            <name>Akshay S Dinesh</name>
            <uri>http://learnlearn.in/about/</uri>
            <email>akshay@learnlearn.in</email>
        </author>
        <content type="html">
            {{ post.content | xml_escape }}
        </content>
    </entry>
    {% endfor %}
</feed>
